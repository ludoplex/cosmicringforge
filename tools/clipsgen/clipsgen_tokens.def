/* clipsgen_tokens.def â€” Business Rules Generator tokens */
TOK(RULE,       "rule",       keyword, "Rule declaration")
TOK(WHEN,       "when",       keyword, "Condition block")
TOK(THEN,       "then",       keyword, "Action block")
TOK(AND,        "and",        keyword, "Logical and")
TOK(OR,         "or",         keyword, "Logical or")
TOK(NOT,        "not",        keyword, "Logical not")
TOK(LBRACE,     "{",          operator, "Block open")
TOK(RBRACE,     "}",          operator, "Block close")
TOK(LPAREN,     "(",          operator, "Expr open")
TOK(RPAREN,     ")",          operator, "Expr close")
TOK(DOT,        ".",          operator, "Member access")
TOK(EQ,         "==",         operator, "Equals")
TOK(NE,         "!=",         operator, "Not equals")
TOK(GT,         ">",          operator, "Greater than")
TOK(LT,         "<",          operator, "Less than")
TOK(ASSIGN,     "=",          operator, "Assignment")
TOK(IDENT,      "[a-zA-Z_][a-zA-Z0-9_]*", identifier, "Identifier")
TOK(NUMBER,     "[0-9]+\\.?[0-9]*",       literal,    "Number")
TOK(STRING,     "\"[^\"]*\"",             literal,    "String")
TOK(WS,         "[ \\t\\n]+", whitespace, "Whitespace")
TOK(COMMENT,    "#[^\\n]*",   whitespace, "Line comment")
