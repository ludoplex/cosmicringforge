/* siggen_tokens.def â€” Function Signature Generator tokens */
TOK(MODULE,     "module",     keyword, "Module declaration")
TOK(FN,         "fn",         keyword, "Function declaration")
TOK(EXTERN,     "extern",     keyword, "External linkage")
TOK(ARROW,      "->",         operator, "Return type")
TOK(LBRACE,     "{",          operator, "Block open")
TOK(RBRACE,     "}",          operator, "Block close")
TOK(LPAREN,     "(",          operator, "Param open")
TOK(RPAREN,     ")",          operator, "Param close")
TOK(LBRACK,     "[",          operator, "Attr open")
TOK(RBRACK,     "]",          operator, "Attr close")
TOK(COLON,      ":",          operator, "Type separator")
TOK(COMMA,      ",",          operator, "List separator")
TOK(STAR,       "*",          operator, "Pointer")
TOK(IDENT,      "[a-zA-Z_][a-zA-Z0-9_]*", identifier, "Identifier")
TOK(WS,         "[ \\t\\n]+", whitespace, "Whitespace")
TOK(COMMENT,    "#[^\\n]*",   whitespace, "Line comment")
